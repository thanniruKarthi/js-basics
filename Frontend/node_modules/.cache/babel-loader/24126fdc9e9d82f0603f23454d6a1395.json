ceRoot":"src/components","sources":["deptTable.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-form\n    ref=\"form\"\n    v-model=\"valid\"\n      lazy-validation>\n      <v-dialog v-model=\"dialog\">\n        <template v-slot:activator=\"{on}\">\n          <v-flex text-right align-right>\n            <v-btn class=\"blue\" v-on=\"on\">\n              <v-icon dark>\n                mdi-plus\n              </v-icon>\n            </v-btn>\n          </v-flex>\n        </template>\n        <v-card>\n          <v-card-text class=\"white\">\n            <v-container>\n              <v-text-field\n      label=\"Empid\"\n      v-model=\"form.empid\"\n      :counter=\"10\"\n      placeholder=\"enter empid\"\n      :rules=\"form.numberRules\"\n      required\n      >\n   \n      </v-text-field>\n      <v-text-field\n      label=\"Name\"\n      v-model=\"form.name\"\n      :counter=\"10\"\n      placeholder=\"enter name\"\n      :rules=\"form.nameRules\"\n      required\n      >\n   \n      </v-text-field>\n\n    <v-text-field\n      v-model=\"form.dept\"\n      :rules=\"form.nameRules\"\n      label=\"dept\"\n      placeholder=\"enter dept\"\n      required\n    ></v-text-field>\n \n        \n        \n    <v-btn\n    :disabled=\"!valid\"\n    color=\"success\"\n    class=\"mr-4\"\n    @click=\"validate\"\n  >\n    Validate\n    </v-btn>\n  </v-container>\n    </v-card-text>\n  </v-card>\n  </v-dialog>\n  </v-form>\n<v-simple-table>\n      <template v-slot:default>\n        <thead>\n          <tr>\n            <th>id</th>\n            <th>empid</th>\n            <th>Name</th>\n            <th>Dept</th>\n            \n\n          </tr>\n        </thead>\n        <tbody class=\"text-left\">\n          <tr v-for=\"item in list\" v-bind:key=\"item.id\">\n            <td>{{item.id}}</td>\n            <td>{{item.empid}}</td>\n            <td>{{item.name}}</td>\n            <td>{{item.dept}}</td>\n         \n            <td>\n              <v-btn class=\"mx-1\" fab dark small color=\"cyan\" @click=\"editItem(item)\" >\n                <v-icon dark>\n                  mdi-pencil\n                </v-icon>\n              </v-btn>\n\n            </td>\n            <td>\n              <v-btn class=\"mx-1\" fab dark small color=\"cyan\" @click=\"deleting(item.id)\" >\n                <v-icon dark>\n                  mdi-delete\n                </v-icon>\n              </v-btn>\n\n            </td>\n\n\n          </tr>\n        </tbody>\n      </template>\n\n    </v-simple-table>\n</v-app>\n</template>\n\n<script>\nconsole.log(process.env.zzz)\n  import Vue from 'vue';\n// import axios from 'axios'\n// import VueAxios from 'vue-axios'\n  export default {\n    name:'SubmitForm',\n    data:(()=>{\n    return{\n      valid: true,\n      list:[],\n      \n     form:{\n      empid:'',\n      name: '',\n      dept:'',\n      nameRules: [\n        v => !!v || 'Name is required',\n        v => (v && v.length <= 10) || 'Name must be less than 10 characters',\n      ],\n     \n      value: [],\n     \n      \n      headers: [\n      {\n        text: 'Id',\n        align: 'start',\n        sortable: true,\n        value: 'id',\n      },\n      { text: 'empId', value: 'empid',sortable:true},\n      { text: 'Name', value: 'name',sortable:true},\n      { text: 'Dept', value: 'dept', sortable: false },\n    ],\n    desserts: [],\n    }\n    }\n  }),\n  mounted() {\n\nVue.axios.get('http://127.0.0.1:40193/readdept')\n\n  .then((resp) => {\n    this.list = resp.data;\n  })\n},\n\n  methods: {\n        validate () {\n          this.$refs.form.validate()\n            this.desserts = [\n        {\n          empid: this.form.name,\n          name: this.form.name,\n          dept:this.form.dept, \n      }\n      ]\n      \n      \n    },\n    \n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}