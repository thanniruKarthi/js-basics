{"ast":null,"code":"import Vue from 'vue';\nimport axios from 'axios';\nimport VueAxios from 'vue-axios';\nVue.use(VueAxios, axios);\nexport default {\n  name: 'api-integration',\n\n  data() {\n    return {\n      operation: '',\n      empid: '',\n      name: '',\n      dept: '',\n      list: undefined,\n      arr: [],\n      dialog: false\n    };\n  },\n\n  mounted() {\n    Vue.axios.get('http://127.0.0.1:40193/readdept').then(resp => {\n      this.list = resp.data;\n    });\n  },\n\n  methods: {\n    async sendData() {\n      await Vue.axios.post(`http://127.0.0.1:40193/createdept`, {\n        empid: this.empid,\n        name: this.name,\n        dept: this.dept\n      });\n      this.dialog = false;\n      Vue.axios.get('http://127.0.0.1:40193/readdept').then(resp => {\n        this.list = resp.data;\n      });\n    },\n\n    async deleting(id) {\n      Vue.axios.delete(`http://127.0.0.1:40193/${id}`);\n      Vue.axios.get('http://127.0.0.1:40193/readdept').then(resp => {\n        this.list = resp.data;\n      });\n    },\n\n    async formediting() {\n      await Vue.axios.put('http://127.0.0.1:40193/updatedept' + this.item.id, this.item.empid, this.item.name, this.item.dept).then(() => {\n        this.mounted();\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+IA;AACA;AACA;AACAA;AACA;EACAC,uBADA;;EAEAC,OACA;IACA;MACAC,aADA;MAEAC,SAFA;MAGAH,QAHA;MAIAI,QAJA;MAKAC,eALA;MAMAC,OANA;MAOAC;IAPA;EASA,CAbA;;EAcAC,UACA;IAEAT,iDAEAU,IAFA,CAEAC,QACA;MACA;IACA,CALA;EAMA,CAvBA;;EAwBAC;IACA,iBACA;MACA;QACAR,iBADA;QAEAH,eAFA;QAGAI;MAHA;MAKA;MACAL,iDAEAU,IAFA,CAEAC,QACA;QACA;MACA,CALA;IAOA,CAhBA;;IAiBA,mBACA;MAGAX;MAEAA,iDAEAU,IAFA,CAEAC,QACA;QACA;MACA,CALA;IAOA,CA9BA;;IA+BA,oBACA;MAEA;QACA;MAEA,CAHA;IAMA;;EAxCA;AAxBA","names":["Vue","name","data","operation","empid","dept","list","arr","dialog","mounted","then","resp","methods"],"sourceRoot":"src/components","sources":["api-integration.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-form methos=\"post\"\n 