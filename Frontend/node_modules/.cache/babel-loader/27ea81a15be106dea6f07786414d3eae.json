equired'],\n\n      choice: [\n       {id :1,name:'math'},{id:2,name:'physics'},{id:3,name:'social'}],\n    \n      items: [\n        'Andhra Pradesh',\n        'Tamil Nadu',\n        'Kerala',\n        'Karnataka',\n        'Goa',\n        'Telangana',\n      ],\n      checkbox: false,\n      radioGroup:1,\n      select:null,\n      subject:[],\n      arr:[],\n      \n     \n    }),\n    \n\n    methods: {\n      save () {\n        //const arr=this.arr\n        this.$refs.form.save()\n        this.arr.push ({\n          name : this.name,\n          email : this.email,\n          gender :this.gender,\n          choice :this.subject,\n          items:this.select,\n        })\n        this.dialog = false\n        this.$refs.form.reset()\n        //console.log(JSON.stringify(arr))\n      },\neditedItem:[],\neditedIndex:-1,\n   \n      Delete(item)\n      {\n        this.editedIndex = this.arr.indexOf(item)\n      this.editedItem = Object.assign({}, item)\n      this.arr.splice(item, 1)\n      },\n      \n      \n  \n    },\n   \n    \n  }\n  \n</script>\n"]},"metadata":{},"sourceType":"module"}