{"ast":null,"code":"import Vue from 'vue';\nimport axios from 'axios';\nimport VueAxios from 'vue-axios';\nVue.use(VueAxios, axios);\nexport default {\n  data() {\n    return {\n      input: {\n        cusid: '',\n        name: '',\n        owner: ''\n      },\n      formDialog: false,\n      nameRules: [v => /^[a-zA-Z]+$/.test(v) || 'Name must be valid'],\n      list: [],\n      flag: true\n    };\n  },\n\n  mounted() {\n    Vue.axios.get('http://127.0.0.1:40193/read').then(resp => {\n      this.list = resp.data;\n      console.log(resp.data);\n    });\n  },\n\n  methods: {\n    async addItem() {\n      flag = true;\n      await Vue.axios.post(`http://127.0.0.1:40193/create`, {\n        empid: this.input.cusid,\n        name: this.input.name,\n        dept: this.input.owner\n      });\n      this.dialog = false;\n      this.mounted();\n      this.$refs.form.reset();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsDA;AACA;AACA;AACAA;AACA;EAEAC;IACA;MACAC;QACAC,SADA;QAEAC,QAFA;QAGAC;MAHA,CADA;MAMAC,iBANA;MAOAC,YACAC,kDADA,CAPA;MAUAC,QAVA;MAWAC;IAXA;EAaA,CAhBA;;EAiBAC;IACAX,6CACAY,IADA,CACAC;MACA;MACAC;IACA,CAJA;EAMA,CAxBA;;EAyBAC;IACA;MACAL;MACA;QACAM,uBADA;QAEAZ,qBAFA;QAGAa;MAHA;MAMA;MACA;MACA;IAEA;;EAbA;AAzBA","names":["Vue","data","input","cusid","name","owner","formDialog","nameRules","v","list","flag","mounted","then","resp","console","methods","empid","dept"],"sourceRoot":"src/components","sources":["customerTable.vue"],"sourcesContent":["<template>\n    <v-app>\n        <div>\n                <v-form refs=\"form\">\n                    <v-dialog width=\"600\" v-model=\"formDialog\">\n\n                        <template v-slot:activator=\"{ on,attr }\">\n                            <v-flex text-right align-right>\n                                <v-btn @click=\"openform\" elevation='5' color=\"primary\" v-bind=\"attr\" v-on=\"on\">\n                                    <v-icon>mdi-plus</v-icon>\n                                </v-btn>\n                            </v-flex>\n\n                        </template>\n                        <v-card text class=\"white\">\n                            <v-text-field v-model=\"input.cusid\" label=\"customer id\"></v-text-field>\n                            <v-text-field v-model=\"input.name\" :rules=\"nameRules\" label=\"customer Name\">\n                            </v-text-field>\n                            <v-text-field v-model=\"input.owner\" :rules=\"nameRules\" label=\"owner\"></v-text-field>\n                            <v-btn v-if=\"flag\" @click=\"addItem\">\n                                submit</v-btn>\n                            <v-btn v-else @click=\"editItem\">\n                                save</v-btn>\n                        </v-card>\n                    </v-dialog>\n                </v-form>\n            </div>\n            <v-simple-table>\n                <thead>\n                    <tr>\n                        <th>id</th>\n                        <th>customerid</th>\n                        <th>name</th>\n                        <th>owner</th>\n                    </tr>\n                </thead>\n                <tr v-for=\"(item,i) in list\" :key=\"i\">\n                    <td>{{item.id}}</td>\n                    <td>{{item.cusid}}</td>\n                    <td>{{item.name}}</td>\n                    <td>{{item.owner}}</td>\n                    <td>\n                        <v-btn @click=\"edit(item)\" color=\"transparent\" fab small elevation=\"0\">\n                            <v-icon small color=\"green\">mdi-pencil</v-icon>\n                        </v-btn>\n                        <v-btn @click=\"remove(item.id)\" color=\"transparent\" fab small elevation=\"0\">\n                            <v-icon small color=\"red\">mdi-delete</v-icon>\n                        </v-btn>\n                    </td>\n                </tr>\n            </v-simple-table>\n    </v-app>\n</template>\n<script>\n    import Vue from 'vue';\n     import axios from 'axios';\n     import VueAxios from 'vue-axios'\nVue.use(VueAxios, axios)\n    export default{\n\n    data() {\n        return {\n            input: {\n                cusid: '',\n                name:'',\n                owner: ''\n            },\n            formDialog: false,\n            nameRules: [\n                v => /^[a-zA-Z]+$/.test(v) || 'Name must be valid'\n            ],\n            list:[],\n            flag:true\n        }\n    },\n    mounted() {\n        Vue.axios.get('http://127.0.0.1:40193/read')\n      .then((resp) => {\n        this.list = resp.data;\n        console.log(resp.data)\n      })\n  \n    },\n    methods:{\n        async addItem() {\n            flag=true\n      await Vue.axios.post(`http://127.0.0.1:40193/create`, {\n        empid: this.input.cusid,\n        name: this.input.name,\n        dept: this.input.owner,\n        \n      })\n      this.dialog = false\n      this.mounted()\n      this.$refs.form.reset()\n\n    }   \n    }\n    }\n</script>"]},"metadata":{},"sourceType":"module"}