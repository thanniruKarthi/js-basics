{"ast":null,"code":"import Vue from 'vue';\nimport axios from 'axios';\nimport VueAxios from 'vue-axios';\nVue.use(VueAxios, axios);\nexport default {\n  name: 'api-integration',\n\n  data() {\n    return {\n      operation: '',\n      list: [],\n      dialog: false,\n      item: {\n        id: null,\n        empid: '',\n        name: '',\n        dept: ''\n      }\n    };\n  },\n\n  mounted() {\n    Vue.axios.get('http://127.0.0.1:40193/readdept').then(resp => {\n      this.list = resp.data;\n    });\n  },\n\n  methods: {\n    async sendData() {\n      await Vue.axios.post(`http://127.0.0.1:40193/createdept`, {\n        empid: this.empid,\n        name: this.name,\n        dept: this.dept\n      });\n      Vue.axios.get('http://127.0.0.1:40193/readdept').then(resp => {\n        this.list = resp.data;\n      });\n      this.item.empid = \"\";\n      this.item.name = \"\";\n      this.item.dept = \"\";\n    },\n\n    async deleting(id) {\n      Vue.axios.delete(`http://127.0.0.1:40193/${id}`);\n      Vue.axios.get('http://127.0.0.1:40193/readdept').then(resp => {\n        this.list = resp.data;\n      });\n    },\n\n    async formedit() {\n      await Vue.axios.put('http://127.0.0.1:40193/updatedept' + this.item.id, this.item.empid, this.item.name, this.item.dept);\n      Vue.axios.get('http://127.0.0.1:40193/readdept').then(resp => {\n        this.list = resp.data;\n      });\n    }\n\n  },\n\n  formediting(item) {\n    this.dialog = true;\n    this.empid = item.empid;\n    this.formedit();\n  }\n\n};","map":{"version":3,"mappings":"AAmJA;AACA;AACA;AACAA;AACA;EACAC,uBADA;;EAEAC,OACA;IACA;MACAC,aADA;MAEAC,QAFA;MAGAC,aAHA;MAIAC;QACAC,QADA;QAEAC,SAFA;QAGAP,QAHA;QAIAQ;MAJA;IAJA;EAWA,CAfA;;EAgBAC,UACA;IAEAV,iDAEAW,IAFA,CAEAC,QACA;MACA;IACA,CALA;EAMA,CAzBA;;EA0BAC;IACA,iBACA;MACA;QACAL,iBADA;QAEAP,eAFA;QAGAQ;MAHA;MAOAT,iDAEAW,IAFA,CAEAC,QACA;QACA;MACA,CALA;MAMA;MACA;MACA;IAEA,CApBA;;IAqBA,mBACA;MACAZ;MAEAA,iDAEAW,IAFA,CAEAC,QACA;QACA;MACA,CALA;IAOA,CAhCA;;IAiCA,iBACA;MACA